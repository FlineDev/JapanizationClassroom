    <script src="/assets/js/components/hs.datepicker.js"></script>
    <script>
      $(document).on('ready', function () {
        // initialization of forms
        $.HSCore.components.HSDatepicker.init('#datepickerDefault, #datepickerInline, #datepickerInlineFrom, #datepickerFrom');
				$( "#datepickerDefault" ).datepicker( "option", "dateFormat", "yy-mm-dd" );
				$( "#datepickerDefault" ).datepicker( "option", "minDate", -365 * 10);
				$( "#datepickerDefault" ).datepicker( "option", "maxDate", 0);
      });
    </script>
